{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "URL",
    "format": {
      "type": "tsv"
    }
  },
  "mark": "line",
  "width": 800,
  "height": 400,
  "encoding": {
    "x": {
      "title": "Time Offset (ms)",
      "field": "timestamp",
      "type": "quantitative",
      "scale": {
        "rangeMin": 0,
        "zero": true
      },
      "impute": {
        "value": 0
      }
    },
    "y": {
      "title": "Request Latency (ms)",
      "field": "value",
      "aggregate": "mean",
      "type": "quantitative",
      "impute": {
        "value": 0
      }
    }
  }
}
