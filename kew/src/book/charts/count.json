{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "URL",
    "format": {
      "type": "tsv"
    }
  },
  "mark": "bar",
  "width": 800,
  "height": 400,
  "encoding": {
    "x": {
      "title": "Time Offset (ms)",
      "field": "timestamp",
      "type": "quantitative",
      "bin": { "step": 50, "extent": [0, 1000] },
      "scale": {
        "zero": true
      }
    },
    "y": {
      "title": "Count",
      "field": "value",
      "aggregate": "sum",
      "type": "quantitative",
      "impute": {
        "value": 0
      }
    }
  }
}
